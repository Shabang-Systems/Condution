{
  "name": "@condution/engine",
  "version": "1.0.0-prealpha.1",
  "description": "The thing that powers the magic that's Condution",
  "main": "dist/CondutionEngine.js",
  "types": "dist/CondutionEngine.d.ts",
  "repository": "https://github.com/Shabang-Systems/CondutionEngine",
  "homepage": "https://future.condution.com",
  "author": "Shabang Systems, and the Condution Authors",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "core:run": "rerun 'emcmake cmake . && make' -i api -i CMakeCache.txt -i CMakeFiles -i Makefile",
    "core:dist": "emcmake cmake . && make",
    "api:run": "nodemon --exec babel-node  --extensions \".ts\" --extensions \".js\" api/CondutionEngine.ts",
    "api:tscomp": "tsc",
    "api:dist": "yarn core:dist && tsc && babel  --extensions \".ts\" --extensions \".js\" api --out-dir dist && cp ./api/CondutionCore.wasm ./dist"
  },
  "prepublish": "tsc && babel  --extensions \".ts\" --extensions \".js\" api --out-dir dist && cp ./api/CondutionCore.wasm ./dist",
  "dependencies": {
    "firebase": "^8.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-rc.1",
    "@babel/core": "^7.0.0-rc.1",
    "@babel/node": "^7.0.0-rc.1",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "nodemon": "^1.18.3",
    "typescript": "^4.1.3"
  }
}
