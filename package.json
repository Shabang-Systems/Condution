{
  "name": "condutionengine",
  "version": "0.0.1",
  "description": "The thing that powers the magic that's Condution",
  "main": "CondutionEngine.js",
  "repository": "https://github.com/Shabang-Systems/CondutionEngine",
  "author": "Shabang Systems, and the Condution Authors",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "api:run": "nodemon --exec babel-node  --extensions \".ts\" --extensions \".js\" api/CondutionEngine.js",
    "core:run": "rerun 'emcmake cmake . && make' -i api -i CMakeCache.txt -i CMakeFiles -i Makefile",
    "api:dist": "babel  --extensions \".ts\" --extensions \".js\" api --out-dir dist && cp ./api/CondutionCore.wasm ./dist"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.0.0-rc.1",
    "@babel/core": "^7.0.0-rc.1",
    "@babel/node": "^7.0.0-rc.1",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "nodemon": "^1.18.3",
    "typescript": "^4.1.3"
  }
}
